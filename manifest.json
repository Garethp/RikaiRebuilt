{
  "manifest_version": 2,
  "name": "RikaiRebuilt",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "48": "icons/border-48.png"
    }
  },

  "permissions": [
    "<all_urls>",
    "tabs"
  ],

  "background": {
    "scripts": [
      "src/config.js",
      "src/FileReader.js",
      "src/deinflectText.js",
      "src/deinflect.js",
      "src/dictionary.js",
      "src/auto-bind.js",
      "src/data.js",
      "src/background.js"
    ]
  }
}
